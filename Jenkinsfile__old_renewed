pipeline {

  agent any

  stages {
    stage ("try jenkinsfile") {
      steps {            
          git branch: "main", url: "https://github.com/zeelz/jenkins.git"
          sh 'docker run node:alpine'
          echo "BUILD_ID is ${BUILD_ID}"
          sh "node -v"
          sh "ls -l"
          sh "npm install"
      }
    }
  }

}
